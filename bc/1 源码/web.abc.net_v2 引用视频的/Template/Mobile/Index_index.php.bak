<script>
    $('#khdd').click( function() {
 
        alert('hh');//这句是为了证明注册点击事件成功，有弹出就说明注册成功。没有弹出的话，检查一下有没有引入jquery的文件。这是jquery方法。
         
        $.ajax({
              cache: false,
              type: "GET",//重要参数1：请求的方式
              url : "ajax_response.php?id=1&num=8",//重要参数2：请求的文件名
              //请求成功执行的function
              success: function(msg){//重要参数3：msg是后台返回的数据，和js的reponsetext一样，如果后台没有返回数据则不会弹出。
                    alert("后台返回数据:"+msg);
                    alert("请求成功");
                 
              },
               //请求成功失败执行的function
              error:function(){
                alert("请求失败");
              }
            });
 
        return false;//重要语句：如果是像a链接那种有href属性注册的点击事件，可以阻止它跳转。
        //就是return后面的语句都不执行了，所以他就不能跳转了。
 
        
    });
 
</script>
<script>
	setNoticeShow('hide')
	var page = 1
	var notice = ""
	window.onload = () => {
		getNotice()
		loadVideo()
		touchBottom()
	}
	
	
	//触底加载
	function touchBottom() {
		$(window).scroll(function() {
			if ($(document).scrollTop()<=0){
				console.log("滚动条已经到达顶部为0");
			}
			if ($(document).height() - $(document).scrollTop() < 900) {
				page ++
				console.log("滚动条已经到达底部为" + $(document).scrollTop());
				loadVideo()
			}
		});
	}
	
	//加载视频
	function loadVideo(){
		$.post('https://video.qlkc998.vip/api/home/videoList', {page: page}, (data) => {
			let res = JSON.parse(data)
			let list = res.data.lists
			if(list.length > 0) {
				list.forEach(el => {
					let html = `
						<div class="videoItem1">
							
							<img src="${el.image}" style="width: 100%;height: 100px;">
							<img src="/Template/Mobile/images/3.png" onclick="handlePlay('${el.id}')" id="playBtn${el.id}" class="play">
							<h3>${el.name}</h3>
							<span>评分：${el.rate}</span>
							<img src="/Template/Mobile/images/2.png" class="cite">
						</div>
					`
					$('#videoBox').append(html)
				})
			}
			
		})
	}
	
    $(document).on('click', '.showCodeC', function () {
        $('.showCodePage').show();
    })
    $(document).on('click', '.hideCodec', function () {
        $('.showCodePage').hide();
    })
    $('li.home_main_list a').click(function (event) {

        event.preventDefault()
        var url = $(this).attr('href');

        $.ajax({
            url: url,
            type: 'POST',
            success: function (json) {

                if (json.sign == 'fase') {
                    window.location.href = "/Public.login.do";
                } else {

                    window.location.href = url;
                }
            }
        })
    })
</script>

